{
  "name": "grammy-callbacks",
  "version": "0.1.0",
  "description": "Callback management system for Grammy Telegram bots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krizka/grammy-callbacks.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint",
    "prepublishOnly": "npm test && npm run build"
  },
  "dependencies": {
    "tiny-invariant": "^1.3.3"
  },
  "peerDependencies": {
    "grammy": "^1.36.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "grammy",
    "telegram",
    "bot",
    "callbacks",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
